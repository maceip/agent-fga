generator client {
  provider = "prisma-client-py"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           String    @id @default(cuid())
  googleSub    String    @unique
  email        String
  accessToken  String
  refreshToken String?
  createdAt    DateTime  @default(now())
}