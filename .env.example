# --- Google OAuth ---
# Get these from your Google Cloud Console
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret

# --- Database (for Marketplace Backend) ---
# This matches the docker-compose service
DATABASE_URL="postgresql://user:password@db:5432/marketplace?schema=public"

# --- Backend Services ---
SESSION_SECRET_KEY=a-very-secure-random-string-for-sessions

# --- Service URLs (for communication between services) ---
# These match the docker-compose service names and ports
PERSONAL_AGENT_URL=http://personal_agent:8002
GOOD_AGENT_URL=http://good_agent:8003
MALICIOUS_AGENT_URL=http://malicious_agent:8004
OPENFGA_API_URL=http://openfga:8080

# --- OpenFGA Store ID ---
# This will be added by the setup script in the README
OPENFGA_STORE_ID=